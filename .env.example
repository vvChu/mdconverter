# mdconverter environment variables
# Copy this file to .env and fill in your values

# Antigravity Proxy URL (for Gemini API access)
MDCONVERT_ANTIGRAVITY_PROXY=http://127.0.0.1:8045

# LlamaCloud API Key (for LlamaParse)
MDCONVERT_LLAMA_CLOUD_API_KEY=

# Gemini API Key (if not using proxy)
MDCONVERT_GEMINI_API_KEY=

# Conversion settings
MDCONVERT_MAX_OUTPUT_TOKENS=65536
MDCONVERT_TIMEOUT_SECONDS=600
MDCONVERT_TEMPERATURE=0.1

# Quality settings
MDCONVERT_MIN_CONTENT_LENGTH=100
MDCONVERT_HIGH_QUALITY_THRESHOLD=95
